{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "analyze": "vue-cli-service build --mode analyze",
    "dev": "vue-cli-service serve --mode dev",
    "new:comp": "node ./scripts/generateComponent",
