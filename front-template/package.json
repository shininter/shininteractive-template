    "vue-router": "^3.0.1",
    "vue-touch": "^2.0.0-beta.4",
    "vuex": "^3.1.0",
    "vuex-along": "^1.2.5",
    "weixin-js-sdk": "^1.4.0-test"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
